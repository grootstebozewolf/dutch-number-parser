{
  "name": "@grootstebozewolf/dutch-number-parser",
  "version": "1.0.0",
  "description": "A TypeScript package to parse and convert Dutch counting numbers into numeric equivalents, supporting numbers up to one million.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "echo \"No tests specified\" && exit 0",
    "prepublishOnly": "npm run build",
    "generate:antlr": "antlr4ts -o src/grammar/generated -lib src/grammar -listener -visitor src/grammar/DutchNumbers.g4"
  },
  "keywords": [
    "antlr",
    "typescript",
    "dutch",
    "numbers",
    "parser",
    "CC BY-SA 4.0"
  ],
  "author": "Jeroen (VAA)",
  "license": "CC-BY-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/grootstebozewolf/dutch-number-parser.git"
  },
  "bugs": {
    "url": "https://github.com/grootstebozewolf/dutch-number-parser/issues"
  },
  "homepage": "https://github.com/grootstebozewolf/dutch-number-parser",
  "publishConfig": {
    "@grootstebozewolf:registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "antlr4ts-cli": "^0.5.0-alpha.4"
  }
}
